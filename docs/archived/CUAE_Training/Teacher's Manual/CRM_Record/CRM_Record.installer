<?xml version="1.0"?>
<install xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://metreos.com/AppInstaller.xsd">
  <configuration>
    <configValue name="phoneUser" displayName="Phone Username" format="String" description="The username of the user associated with the phone" defaultValue="metreos" required="true" />
    <configValue name="phonePass" displayName="Phone Password" format="Password" description="The password of the user associated with the phone" defaultValue="metreos" required="true" />
    <configValue name="dialin" displayName="Dial In DN" format="String" description="The dial in trigger extension" defaultValue="" required="true" />
  </configuration>
</install>