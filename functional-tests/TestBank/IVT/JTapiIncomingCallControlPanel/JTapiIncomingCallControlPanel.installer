<?xml version="1.0"?>
<install xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://metreos.com/AppInstaller.xsd">
  <configuration>
    <configValue name="IVT_JTapiIncomingCallControlPanel_script1" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1" />
    <configValue name="S_AnswerCallComplete" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.S_AnswerCallComplete" />
    <configValue name="S_AnswerCallFail" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.S_AnswerCallFail" />
    <configValue name="S_Hangup" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.S_Hangup" />
    <configValue name="S_Conference" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.S_Conference" />
    <configValue name="E_Hangup" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.E_Hangup" />
    <configValue name="E_Conference" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.E_Conference" />
    <configValue name="S_BlindSuccess" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.S_BlindSuccess" />
    <configValue name="S_BlindFail" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.S_BlindFail" />
    <configValue name="S_ConferenceSuccess" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.S_ConferenceSuccess" />
    <configValue name="S_ConferenceFail" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.S_ConferenceFail" />
    <configValue name="E_Blind" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.E_Blind" />
    <configValue name="E_IncomingCall" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.E_IncomingCall" />
    <configValue name="S_IncomingCall" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.S_IncomingCall" />
    <configValue name="ReversionTime" displayName="Reversion Time" format="Number" description="Number of seconds that a held call will wait before before it rings back the person who put the call on hold" minValue="0" maxValue="1200" defaultValue="0" />
    <configValue name="Username" displayName="Username" format="String" description="Name of CCM user with rights to control all phone devices which will be used with this application" defaultValue="metreos" />
    <configValue name="Password" displayName="Password" format="Password" description="Password Password of CCM user" defaultValue="metreos" />
    <configValue name="Filename" displayName="Filename" format="String" description="File to play as the tariff tone" defaultValue="chime.raw" />
    <configValue name="S_CallActive" format="String" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.S_CallActive" />
    <configValue name="E_UpdateGlobals" displayName="" format="String" description="" defaultValue="IVT.JTapiIncomingCallControlPanel.script1.E_UpdateGlobals" />
  </configuration>
</install>