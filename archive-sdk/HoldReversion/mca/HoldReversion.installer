<?xml version="1.0"?>
<install xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://metreos.com/AppInstaller.xsd">
  <configuration>
    <configValue name="ReversionTime" displayName="Reversion Time" format="Number" description="Number of seconds that a held call will wait before before it rings back the person who put the call on hold" minValue="0" maxValue="1200" defaultValue="0" required="false" />
    <configValue name="Username" displayName="Username" format="String" description="Name of CCM user with rights to control all phone devices which will be used with this application" defaultValue="metreos" required="true" />
    <configValue name="Password" displayName="Password" format="Password" description="Password Password of CCM user" defaultValue="metreos" required="true" />
    <configValue name="Filename" displayName="Filename" format="String" description="File to play as the tariff tone" defaultValue="chime.raw" required="true" />
  </configuration>
</install>